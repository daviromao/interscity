<p id="notice"><%= notice %></p>

<h1>Data</h1>

<table>
  <thead>
    <tr>
      <th>Component uuid</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Capability</th>
      <th>Attribute</th>
      <th>Type</th>
      <th>Unity</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.component_uuid %></td>
        <td><%= datum.lat %></td>
        <td><%= datum.lon %></td>
        <td><%= datum.capability %></td>
        <td><%= datum.attribute %></td>
        <td><%= datum.type %></td>
        <td><%= datum.unity %></td>
        <td><%= datum.value %></td>
        <td><%= link_to 'Show', datum %></td>
        <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
        <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datum', new_datum_path %>
